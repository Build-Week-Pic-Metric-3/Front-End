{
  "version": 2,
  "name": "pic-metric-3",
  "builds": [
      {
          "src": "pic-metric-3/package.json",
          "use": "@now/static-build",
          "config": {
              "distDir": "build"
          }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "pic-metric-3/$1"
      }
  ]
}